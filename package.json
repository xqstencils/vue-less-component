{
  "name": "vue-less-component",
  "version": "1.0.0",
  "description": "Vue Component",
  "main": "dist/index.js",
  "module": "es/index.esm.js",
  "directories": {
    "example": "example"
  },
  "scripts": {
    "start": "webpack serve --config config/webpack.config.js --open",
    "clean": "rimraf lib && rimraf es && rimraf dist",
    "build": "yarn clean && yarn build:dist && yarn build:dist-min && yarn build:esm && yarn build:esm-bundle",
    "build:dist": "cross-env LIBMODE=core webpack build --config ./config/webpack.config.umd.js",
    "build:dist-min": "cross-env LIBMODE=core webpack build --config ./config/webpack.config.umd-min.js",
    "build:esm": "cross-env LIBMODE=core rollup --config ./config/rollup.config.es.js",
    "build:esm-bundle": "cross-env LIBMODE=core rollup --config ./config/rollup.config.bundle.js",
    "test": "yarn lint && yarn test:unit",
    "lint": "eslint src",
    "test:unit": "jest --coverage --config ./config/jest.config.js"
  },
  "dependencies": {},
  "peerDependencies": {
    "vue": "^2.6.12"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-export-default-from": "^7.12.13",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@testing-library/dom": "^7.30.0",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/vue": "^5.6.1",
    "@vue/babel-plugin-jsx": "^1.0.3",
    "@vue/eslint-config-standard": "^6.0.0",
    "@vue/test-utils": "^1.1.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-preset-vue": "^2.0.2",
    "cross-env": "^7.0.3",
    "css-loader": "^5.1.3",
    "eslint": "^7.22.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-vue": "^7.7.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.1",
    "jest": "^26.6.3",
    "less": "^4.1.1",
    "less-loader": "^8.0.0",
    "lint-staged": "^10.5.4",
    "postcss": "^8.2.8",
    "rimraf": "^3.0.2",
    "rollup": "^2.42.1",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^5.1.9",
    "style-loader": "^2.0.0",
    "url-loader": "^4.1.1",
    "vue": "^2.6.12",
    "vue-jest": "^3.0.7",
    "vue-loader": "^15.9.6",
    "vue-style-loader": "^4.1.3",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^5.26.3",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  },
  "files": [
    "es",
    "lib",
    "src",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xqstencils/vue-less-component.git"
  },
  "author": "XiaoqiCao<xqcao@xqcao.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xqstencils/vue-less-component/issues"
  },
  "homepage": "https://github.com/xqstencils/vue-less-component#readme"
}
